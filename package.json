{
    "name": "ms_ocr",
    "version": "1.0.0",
    "description": "Microservice OCR.",
    "author": "cimo",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/cimo/Ms_ocr.git"
    },
    "scripts": {
        "preinstall": "([ ! -f package-lock.json ] && npm install --package-lock-only --ignore-scripts --no-audit); npx npm-force-resolutions",
        "remove_dist": "node -e \"const fs = require('fs'); try{ fs.rmdirSync('./dist/', { recursive: true }) } catch{}; process.exit(0);\"",
        "check": "eslint --no-cache --config eslint.config.js --fix",
        "build": "npm run remove_dist && npm run check && tsc --build tsconfig.build.json && npm audit",
        "execute": "nodemon --config nodemon.json"
    },
    "dependencies": {
        "@cimo/authentication": "1.1.16",
        "@cimo/form-data_parser": "1.1.22",
        "@cimo/environment": "1.0.7",
        "express": "4.21.2",
        "express-rate-limit": "7.5.0",
        "cookie-parser": "1.4.7",
        "cors": "2.8.5"
    },
    "devDependencies": {
        "typescript": "5.7.2",
        "eslint": "9.20.1",
        "@typescript-eslint/parser": "8.24.1",
        "@typescript-eslint/eslint-plugin": "8.24.1",
        "eslint-plugin-prettier": "5.2.1",
        "globals": "15.13.0",
        "prettier": "3.4.2",
        "nodemon": "3.1.9",
        "@types/node": "22.10.2",
        "@types/express": "5.0.0",
        "@types/cookie-parser": "1.4.8",
        "@types/cors": "2.8.17"
    },
    "resolutions": {
        "node-fetch": "2.6.7",
        "**/node-fetch": "2.6.7",
        "crypto-js": "4.2.0",
        "**/crypto-js": "4.2.0",
        "path-to-regexp": "0.1.12",
        "**/path-to-regexp": "0.1.12",
        "send": "0.19.0",
        "**/send": "0.19.0",
        "serve-static": "1.16.0",
        "**/serve-static": "1.16.0",
        "body-parser": "1.20.3",
        "**/body-parser": "1.20.3",
        "cookie": "0.7.0",
        "**/cookie": "0.7.0"
    }
}
