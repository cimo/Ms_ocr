{
    "name": "ms_ocr",
    "version": "1.0.0",
    "description": "Microservice OCR.",
    "author": "cimo",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/cimo/Ms_ocr.git"
    },
    "main": "./src/Controller/Server.ts",
    "scripts": {
        "preinstall": "npm install --package-lock-only --ignore-scripts --no-audit && npx npm-force-resolutions@0.0.3",
        "check": "eslint . --ext .ts --fix",
        "link": "npm link /home/root/project/form-data_parser/root/",
        "execute-ow": "nodemon --watch src --ext ts --exec 'tsc && node --require dotenv/config ./dist/Controller/Server.js dotenv_config_path=./env/local-ow.env'",
        "execute": "nodemon --watch src --ext ts --exec 'tsc && node ./dist/Controller/Server.js'",
        "build": "tsc --build -clean && tsc --build"
    },
    "dependencies": {
        "express": "4.18.1",
        "cookie-parser": "1.4.6",
        "cors": "2.8.5",
        "@cimo/form-data_parser": "1.1.2"
    },
    "devDependencies": {
        "typescript": "4.9.4",
        "nodemon": "2.0.21",
        "dotenv": "16.0.3",
        "eslint": "8.35.0",
        "prettier": "2.8.7",
        "@types/node": "18.14.4",
        "@typescript-eslint/parser": "5.54.0",
        "@typescript-eslint/eslint-plugin": "5.54.0",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-config-prettier": "8.8.0",
        "@types/express": "4.17.17",
        "@types/cookie-parser": "1.4.3",
        "@types/cors": "2.8.13"
    },
    "resolutions": {
        "node-fetch": "2.6.7",
        "**/node-fetch": "2.6.7"
    }
}
